<!--
    Layoutfiles, describes how a bunch of pages should look
    We can have 1 file, which is a template layout file that's applied
    to every individual page
 -->
{{define "base"}}
<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8" />
    <title>Home</title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD"
      crossorigin="anonymous"
    />
    {{block "css" .}}
    {{
      end
    }}
  </head>
  <body>
    {{block "content" .}}
    {{ end }}
    {{block "js" .}}
    {{ end }}
  </body>
</html>
{{ end }}
